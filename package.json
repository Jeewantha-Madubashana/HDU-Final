{
  "name": "hdu-final",
  "version": "1.0.0",
  "description": "HDU Final Project - Client and Server",
  "private": true,
  "scripts": {
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "install:client": "cd client && npm install",
    "install:server": "cd server && npm install",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:client": "cd client && npm run dev",
    "start:server": "cd server && npm start",
    "build": "cd client && npm run build",
    "flush-db": "node server/scripts/flushAllTables.js",
    "init-db": "node server/scripts/initializeDatabase.js",
    "init-vital-signs": "node server/scripts/initializeVitalSignsConfig.js",
    "add-urgent-columns": "node server/scripts/addUrgentAdmissionColumns.js",
    "reset-and-start": "node server/scripts/flushAllTables.js && node server/scripts/initializeVitalSignsConfig.js && node server/scripts/initializeDatabase.js && concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "reset": "node server/scripts/flushAllTables.js && node server/scripts/initializeVitalSignsConfig.js && node server/scripts/initializeDatabase.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.0"
  },
  "dependencies": {
    "canvas": "^3.2.0",
    "docx": "^9.5.1",
    "pdfkit": "^0.17.2"
  }
}
